<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:paddingLeft="30dp"
    android:paddingRight="30dp"
    tools:context=".PosImportacao" >

    <TextView
        android:id="@+id/textViewMsgPosImport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:gravity="left"
        android:paddingTop="40dp"
        android:text="@string/informacoes_importadas"
        android:textSize="17sp" />
    <TextView
        android:id="@+id/textViewDadosImportados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:gravity="left"
        android:paddingTop="15dp"
        android:textSize="15sp" />
    
     <Button
        android:id="@+id/continuar_preencher_perfil"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="@drawable/botao_configuracoes"
        android:text="@string/continuar_preenchendo_o_perfil"
        android:textColor="#FFFFFF" />

</LinearLayout>