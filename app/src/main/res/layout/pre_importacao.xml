<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:paddingLeft="30dp"
    android:paddingRight="30dp"
    tools:context=".PreImportacao" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:paddingTop="40dp"
        android:text="@string/importar_dados_facebook"
        android:textSize="17sp" />

    <Button
        android:id="@+id/saiba_mais"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="@drawable/botao_configuracoes"
        android:text="@string/saiba_mais"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/importar_agora_pre"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="@drawable/botao_configuracoes"
        android:text="@string/importar_agora"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/nao_importar"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="@drawable/botao_configuracoes"
        android:text="@string/nao_importar"
        android:textColor="#FFFFFF" />

</LinearLayout>